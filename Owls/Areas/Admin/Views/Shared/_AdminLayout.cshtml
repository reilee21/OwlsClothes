<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="apple-touch-icon" sizes="76x76" href="~/image/icon.png">
    <link rel="icon" type="image/png" href="~/image/icon.png">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/morris.js/0.5.1/morris.css">

    <title>
        Owls Admin  
    </title>
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,400,600,700" rel="stylesheet" />
    <link asp-append-version="true" href="~/css/nucleo-icons.css" rel="stylesheet" />
    <link asp-append-version="true" href="~/css/nucleo-svg.css" rel="stylesheet" />
    <script src="https://kit.fontawesome.com/42d5adcbca.js" crossorigin="anonymous"></script>
    <link asp-append-version="true" href="~/css/nucleo-svg.css" rel="stylesheet" />
    <link asp-append-version="true" href="~/css/admin.css" rel="stylesheet" />
    <link asp-append-version="true" href="~/css/site.css" rel="stylesheet" />

    <link id="pagestyle" asp-append-version="true" href="~/css/soft-ui-dashboard.css?v=1.0.3" rel="stylesheet" />
</head>

<body class="g-sidenav-show  bg-gray-100">
    <partial name="_SideBar"/>
    <main class="main-content position-relative max-height-vh-100 h-100 mt-1 border-radius-lg ">
    <partial name="_Nav.cshtml"/>
        <div class="container-fluid py-4">
          @RenderBody()
        </div>
    </main>
    <script asp-append-version="true" src="~/js/jquery-3.3.1.min.js"></script>

    <script asp-append-version="true" src="~/js/popper.min.js"></script>
    <script asp-append-version="true" src="~/js/bootstrap.min.js"></script>
    <script asp-append-version="true" src="~/js/perfect-scrollbar.min.js"></script>
    <script asp-append-version="true" src="~/js/smooth-scrollbar.min.js"></script>
    <script>
        var win = navigator.platform.indexOf('Win') > -1;
        if (win && document.querySelector('#sidenav-scrollbar')) {
            var options = {
                damping: '0.5'
            }
            Scrollbar.init(document.querySelector('#sidenav-scrollbar'), options);
        }
    </script>
    <script async defer src="https://buttons.github.io/buttons.js"></script>
    <script asp-append-version="true" src="~/js/soft-ui-dashboard.min.js?v=1.0.3"></script>
    <partial name="_ValidationScriptsPartial.cshtml"/>
    @RenderSection("Scripts", required: false)
</body>

</html>