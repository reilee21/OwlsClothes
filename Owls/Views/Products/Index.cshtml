@model ProductListVM

<partial name="Filter"/>
@if(Model.Products.Count() > 0)
{
	<div class="row">
		@foreach (var product in Model.Products)
		{
			<div class="col-xs-6 col-sm-6 col-md-6 col-lg-3">

				<partial name="_ProductCard" model="@product"/>
			</div>

		}
	</div>
	<partial name="Pagination" />
}


